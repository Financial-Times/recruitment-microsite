doctype html
html(lang="en")
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title Product and Tech
    link(rel='stylesheet', href='css/main.css', type='text/css')
  body
    div.header
        nav.menu
            each val in menu.items
                li.menu__item
                    a(href=`index.html#${val.link}`)=val.text
    section.intro.thankyou
        img.heritage-logo(src=`images/${intro.heritage_logo}`, alt=`{intro.heritage_logo}`, role='presentation')
        div.title
            h1 Thank you
            hr
            p A member of our team will be in contact shortly
